{
  "name": "mrpromth-production",
  "version": "1.0.0",
  "private": true,
  "description": "Mr.Promth Production - AI-powered screenshot-to-code platform",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "backend:dev": "pnpm --filter @mrpromth/backend dev",
    "extension:dev": "pnpm --filter @mrpromth/extension dev",
    "extension:build": "pnpm --filter @mrpromth/extension build"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "turbo": "^1.10.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
